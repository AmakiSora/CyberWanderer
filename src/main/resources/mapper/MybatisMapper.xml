<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.MybatisDao">
    <insert id="insertBiliBiliDynamic" parameterType="BiliBili.pojo.BiliBiliDynamic">
        insert ignore into dynamic
        (NO,id,name,type,uploadTime,content,imgURL)
        values(#{NO},#{id},#{name},#{type},#{uploadTime},#{content},#{imgURL})
    </insert>
    <insert id="insertBiliBiliUser" parameterType="BiliBili.pojo.BiliBiliUser">
        insert ignore into akiUser
            (id,name,avatarURL)
        values(#{id},#{name},#{avatarURL})
    </insert>

</mapper>